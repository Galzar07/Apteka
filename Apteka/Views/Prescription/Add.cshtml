@model PrescriptionViewModel

@{
    ViewData["Title"] = "Prescription Page";
}

<form asp-controller="Prescription" asp-action="Add">
    Name: <input name="Name" type="text" /> <br />
    CreatedDate: <input name="CreatedDate" type="date" /> <br />
    <button type="submit" class="btn btn-success">Add</button>
</form>


